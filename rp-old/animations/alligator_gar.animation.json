{
	"format_version": "1.8.0",
	"animations": {
		"animation.alligator_gar.swim": {
                	"anim_time_update": "query.modified_distance_moved",
			"loop": true,
			"bones": {
				"head": {
					"rotation": [0, "math.cos(-50+query.anim_time*50)*10", 0]
				},
				"tail": {
					"rotation": [0, "math.cos(query.anim_time*50)*20 + query.target_y_rotation", 0]
				},
				"tail_tip": {
					"rotation": [0, "math.cos(-40+query.anim_time*50)*20 + query.target_y_rotation", 0]
				},
				"body": {
					"rotation": [0, "math.cos(-40+query.anim_time*50)*-5", 0]
				}
			}
		}
	}
}